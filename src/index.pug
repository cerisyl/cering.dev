extends ./templates/default.pug

block meta
    - const title = "home"
    - const desc = "The official website of Spencer \"CERiNG\" Gunning- a multidisciplinary designer and programmer specializing in UI/UX, graphics, and game design."

block content
    h1.extend-small modfiles + itg
    .container
        +thin("https://github.com/aemx/mod-releases/raw/main/packs/NJSRT1.zip", "01", "njsrt")
        +thin("https://github.com/aemx/mod-releases/raw/main/packs/Misfits%20in%20the%20Prairie.zip", "02", "misfits in the prairie")
        +thin("https://github.com/aemx/mod-releases/raw/main/packs/Aster's%20Astral%20Adventure.zip", "03", "aster's astral adventure")
        p looking for something else? check out the full list of my #[a.external(href="https://github.com/aemx/mod-releases" target="_blank" rel="noopener") mod releases] and #[a.external(href="https://github.com/aemx/itg-releases" target="_blank" rel="noopener") itg releases].
    h1.extend-small professional work
    .container
        +thin("./img/comms24.webp", "04", "commission information: multidisciplinary design on demand")
        +thin("http://raritanlibrary.org/", "05", "web development at raritan public library")
        +thin("https://arcaea.lowiro.com/", "06", "level design at lowiro limited")
        +thin("https://cering.bandcamp.com/", "07", "musical works")
    h1.extend-none contact me
    p you can reach out to me through #[a.external(href="https://discordid.netlify.app/?id=210385938918932480" target="_blank" rel="noopener") discord], #[a.external(href="https://twitter.com/asteradd" target="_blank" rel="noopener") twitter], or via #[a(href="mailto:hello@cering.dev") email].